<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="../assets/img/darth-vader.png" />
	  <link rel="icon" type="image/png" href="../assets/img/darth-vader1.png" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    <title>
      <%=title%>
    </title>

    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.1/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.1/angular-resource.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.1/angular-route.min.js"></script>

    <!-- Bootstrap core CSS     -->
    <link href="../assets/css/bootstrap.min.css" rel="stylesheet" />

    <!--  Material Dashboard CSS    -->
    <link href="../assets/css/material-dashboard.css" rel="stylesheet"/>

    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="../assets/css/demo.css" rel="stylesheet" />

    <!--     Fonts and icons     -->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300|Material+Icons' rel='stylesheet' type='text/css'>
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
	

</head>

  <body ng-controller="myCtrl">
    <div id="log" style="display:none;">
      <%=parserLog%>
    </div>
    <div class="wrapper">
      <div class="sidebar" data-color="purple" data-image="../assets/img/sidebar-1.jpg">

        <div class="logo">
          <a href="http://filipemendes.droppages.com/" target="_blank" class="simple-text">
					  Filipe Mendes
				  </a>
        </div>

        <div class="sidebar-wrapper">
          <ul class="nav">
            <li class="active">
              <a href="#">
                <i class="material-icons">bubble_chart</i>
                <p>Desafio Wunderman</p>
              </a>
            </li>
          </ul>
        </div>
      </div>

      <div class="main-panel" ng-switch="showDiv">
        <div class="col-lg-3 col-md-6 col-sm-6" style="margin-left: 13px;">
          <div class="card card-stats">
            <div class="card-header" data-background-color="red">
              <i class="material-icons">info_outline</i>
            </div>
            <div class="card-content" ng-repeat="i in games" ng-if="$last">
              <p class="category">Total de Partidas</p>
              <h3 class="title">{{i.total_game}}</h3>
            </div>
          </div>
        </div>
        <div class="content">
          <div class="container-fluid">
            <div class="row">
              <div class="col-md-12">
                <div class="card">
                  <div class="card-header" data-background-color="purple">
                    <h4 class="title">Teste prático de Backend</h4>
                    <p class="category">Quake log parser</p>
                  </div>
                  <div class="card-content table-responsive" ng-switch-default="">
                    <table class="table">
                      <thead class="text-primary">
                        <th>Partida Nº</th>
                        <th>Total de Mortes</th>
                        <th>Jogadores Conectados </th>
                        <th>Players / Mortes Causadas</th>
                        <th>Players / Mortes Sofridas</th>
                        <th>Kills Means</th>
                      </thead>
                      <tbody ng-repeat="game in games">
                        <tr>
                          <td>
                            <b>{{game.games}}</b>
                          </td>
                          <td>{{game.total_kills}}</td>
                          <td>{{game.total_players}}</td>
                          <td><table ng-repeat="(key, val) in game.players"><tr><td>{{val}}: {{key}}</tr></td></table></td>
                          <td><table ng-repeat="(key, val) in game.kills"><tr><td>{{key}}: {{val}}</tr></td></table></td>
                          <td><table ng-repeat="(key, val) in game.killsMeans"><tr><td>{{key}}: {{val}}</tr></td></table></td>
                        </tr>
                    </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>

        <footer class="footer">
              <div class="container-fluid">
                  <nav class="pull-left">
                      <ul>
                          <li>
                              <a href="http://bit.ly/2cIL9ur" target="_blank">
                                  Website
                              </a>
                          </li>
                          <li>
                              <a href="https://www.linkedin.com/in/filipe-mendes-76aa4a78/" target="_blank">
                                  LinkedIn
                              </a>
                          </li>
                          <li>
                              <a href="https://www.behance.net/FilipeMoreiraMendes" target="_blank">
                                  Portfólio
                              </a>
                          </li>
                          <li>
                              <a href="https://github.com/MendesFilipe" target="_blank">
                                  GitHub
                              </a>
                          </li>
                      </ul>
                  </nav>
              </div>
        </footer>
      </div>
    </div>
  </body>

  <!--   Core JS Files   -->
  <script src="../assets/js/jquery-3.1.0.min.js" type="text/javascript"></script>
  <script src="../assets/js/bootstrap.min.js" type="text/javascript"></script>
  <script src="../assets/js/material.min.js" type="text/javascript"></script>

  <!--  Charts Plugin -->
  <script src="../assets/js/chartist.min.js"></script>

  <!--  Notifications Plugin    -->
  <script src="../assets/js/bootstrap-notify.js"></script>

  <!--  Google Maps Plugin    -->
  <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script>

  <!-- Material Dashboard javascript methods -->
  <script src="../assets/js/material-dashboard.js"></script>

  <!-- Material Dashboard DEMO methods, don't include it in your project! -->
  <script src="../assets/js/demo.js"></script>

  <script src="js/parserLog.js"></script>
  <script src="js/project.js"></script>

</html>
